{
  "name": "politico-react",
  "version": "1.0.0",
  "description": "This is the front-end part of my Politico project converted to React.js",
  "main": "index.js",
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "verbose": true,
    "testURL": "http://localhost:8080/",
    "setupFiles": [
      "<rootDir>/src/setupTests.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/assetsTransformer.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.scss$": "identity-obj-proxy"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.jsx": [
      "npm run lint-fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:coverage": "jest --coverage --colors",
    "coverage": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development --open --hot",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raymond-Osy/Politico-React.git"
  },
  "author": "Raymond Akalonu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raymond-Osy/Politico-React/issues"
  },
  "homepage": "https://github.com/Raymond-Osy/Politico-React#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.13",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.5",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "react-testing-library": "^7.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "express": "^4.17.1",
    "jest-localstorage-mock": "^2.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-redux-toastr": "^7.5.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "validator": "^11.0.0"
  }
}
